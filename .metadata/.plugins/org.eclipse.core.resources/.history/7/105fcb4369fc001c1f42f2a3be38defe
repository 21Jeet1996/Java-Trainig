package com.userFlight.kafka.model;

public class UserFlight {

	private Long pnr;
	private String email;
	private String bookingStatus;
	private int totalBooking;
	
	
	public UserFlight() {
		super();
	}


	public UserFlight(Long pnr, String email, String bookingStatus, int totalBooking) {
		super();
		this.pnr = pnr;
		this.email = email;
		this.bookingStatus = bookingStatus;
		this.totalBooking = totalBooking;
	}


	public Long getPnr() {
		return pnr;
	}


	public void setPnr(Long pnr) {
		this.pnr = pnr;
	}


	public String getEmail() {
		return email;
	}


	public void setEmail(String email) {
		this.email = email;
	}


	public String getBookingStatus() {
		return bookingStatus;
	}


	public void setStatus(String bookingStatus) {
		this.bookingStatus = bookingStatus;
	}


	public int getTotalBooking() {
		return totalBooking;
	}


	public void setTotalBooking(int totalBooking) {
		this.totalBooking = totalBooking;
	}


	@Override
	public String toString() {
		return "UserFlight [pnr=" + pnr + ", email=" + email + ", status=" + bookingStatus + ", totalBooking=" + totalBooking
				+ "]";
	}
	
	
	
}
